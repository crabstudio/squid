{
  "name": "create-squid-app",
  "version": "0.0.6",
  "description": "CLI tool for creating squid application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crabstudio/squid.git",
    "directory": "cli"
  },
  "keywords": [
    "create-squid-app",
    "cli"
  ],
  "author": "crabstudio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crabstudio/squid/issues"
  },
  "homepage": "https://github.com/crabstudio/squid#readme",
  "main": "index.ts",
  "exports": "./dist/index.js",
  "bin": {
    "create-squid-app": "./dist/index.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist node_modules",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "pub:beta": "pnpm build && npm publish --tag beta",
    "pub:release": "pnpm build && npm publish"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "chalk": "^5.2.0",
    "chalk-animation": "^2.0.3",
    "commander": "^10.0.0",
    "figlet": "^1.6.0",
    "inquirer": "^9.1.5",
    "ora": "^6.3.0"
  },
  "devDependencies": {
    "@types/chalk-animation": "^1.6.1",
    "@types/figlet": "^1.5.5",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.15.11",
    "typescript": "^5.0.4"
  }
}