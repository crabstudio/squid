{
  "name": "@squid/root",
  "version": "0.0.2",
  "description": "Start your backend project with the most simple and easy way",
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@8.2.0",
  "type": "module",
  "scripts": {
    "start:cli": "cd cli && pnpm start",
    "build:cli": "cd cli && pnpm build",
    "dev:cli": "cd cli && pnpm dev",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "prepare": "husky install",
    "pub:beta": "cd cli && pnpm pub:beta",
    "pub:release": "cd cli && pnpm pub:release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crabstudio/squid.git"
  },
  "keywords": [
    "API",
    "RESTAPI",
    "endpoint",
    "framework",
    "create-squid-app"
  ],
  "author": "crabstudio",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/crabstudio/squid/issues"
  },
  "homepage": "https://github.com/crabstudio/squid#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3"
  }
}