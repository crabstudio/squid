{
  "name": "squid",
  "version": "0.0.1",
  "description": "Start your backend project with the most simple and easy way",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start:cli": "node --experimental-specifier-resolution=node --loader ts-node/esm cli/index.ts",
    "dev": "nodemon --experimental-specifier-resolution=node --loader ts-node/esm cli/index.ts",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crabstudio/squid.git"
  },
  "keywords": [
    "API",
    "RESTAPI",
    "endpoint",
    "framework",
    "create-squid-app"
  ],
  "author": "crabstudio",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/crabstudio/squid/issues"
  },
  "homepage": "https://github.com/crabstudio/squid#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "typescript": "^5.0.3"
  }
}